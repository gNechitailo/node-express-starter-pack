{
  "name": "myApp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "env-cmd -f ./env/development.env nodemon ./bin/www",
    "migrate-up": "env-cmd -f ./env/development.env sequelize db:migrate",
    "migrate-prod-up": "sequelize db:migrate",
    "migrate-down": "env-cmd -f ./src/env/development.env sequelize db:migrate:undo",
    "migrate-down-all": "env-cmd -f ./src/env/development.env sequelize db:migrate:undo:all",
    "pretest": "eslint --ignore-path .gitignore . --fix"
  },
  "dependencies": {
    "apollo-server": "^2.9.13",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "elasticsearch": "^16.5.0",
    "env-cmd": "^10.0.1",
    "express": "~4.16.1",
    "facebook-login": "^1.4.0",
    "google-oauth2": "^0.1.0",
    "graphql": "^14.5.8",
    "http-errors": "~1.6.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pug": "2.0.0-beta11",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "stripe": "^7.14.0",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3",
    "uuidv4": "^6.0.0",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^2.7.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^1.19.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretest"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix",
      "git add"
    ]
  }
}
