{
  "name": "node-express-starter-pack",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "env-cmd -f .env nodemon ./bin/www",
    "devstart-cron": "env-cmd -f .env node ./bin/start-cron.js",
    "devstart-pm2": "env-cmd -f .env pm2 start ./ecosystem.config.js",
    "migrate-up": "env-cmd -f .env sequelize db:migrate",
    "migrate-prod-up": "sequelize db:migrate",
    "migrate-down": "env-cmd -f .env sequelize db:migrate:undo",
    "migrate-down-all": "env-cmd -f .env sequelize db:migrate:undo:all",
    "pretest": "eslint --ignore-path .gitignore . --fix"
  },
  "dependencies": {
    "awilix": "^4.2.5",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "env-cmd": "^10.0.1",
    "express": "~4.16.1",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.3.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "pm2": "^5.2.0",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3",
    "ws": "^7.2.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^2.7.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^1.19.4"
  },
  "engines": {
    "node": ">=12.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretest"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix",
      "git add"
    ]
  }
}
